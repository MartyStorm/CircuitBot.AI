{
  "name": "circuitbot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n FE,BE -c green,blue \"npm:dev:fe\" \"npm:dev:be\"",
    "dev:fe": "vite",
    "dev:be": "npm run dev --prefix server",
    "dev:all": "concurrently -n FE,BE -c green,blue \"npm:dev:fe\" \"npm:dev:be\"",
    "build": "vite build",
    "build:all": "npm run build && npm run install:server",
    "install:server": "npm install --prefix server",
    "start": "npm run install:server && node server/index.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^5.4.10"
  },
  "overrides": {}
}

